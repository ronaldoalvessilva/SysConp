	<queryString>
		<![CDATA[SELECT B.IdEvolucaoFam, B.DataEvolVisita, B.UsuarioInsert, P.NomeInternoCrc, P.Cnc FROM dbo.EVOLUCAO_ATENDIMENTO_FAMILIA B
INNER JOIN ATENDIMENTOFAMILIAR C ON B.IdAtendf=C.IdAtendf
INNER JOIN PRONTUARIOSCRC P ON C.IdInternoCrc=P.IdInternoCrc
WHERE B.DataEvolVisita BETWEEN $P{dataInicial} AND $P{dataFinal}
UNION ALL
SELECT A.IdAtend, A.DataEvol, A.UsuarioInsert, P.NomeInternoCrc, P.Cnc FROM EVOLUCAO_ATENDIMENTO_SOCIAL A
INNER JOIN PRONTUARIOSCRC P ON A.IdInternoCrc=P.IdInternoCrc
WHERE DataEvol BETWEEN $P{dataInicial} AND $P{dataFinal}
ORDER BY B.UsuarioInsert, B.DataEvolVisita]]>
	</queryString>
