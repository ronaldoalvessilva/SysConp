	<queryString>
		<![CDATA[SELECT
     A."IdLanc",
     A."DataLanc",
     A."UsuarioInsert",
     A."UsuarioUp",
     P."NomeInternoCrc",
     P."Cnc"
FROM
     "ADMISSAOMEDICA" A
	 INNER JOIN "PRONTUARIOSCRC" P ON A."IdInternoCrc" = P."IdInternoCrc"
	 WHERE
     A.DataLanc BETWEEN $P{dataInicial} AND $P{dataFinal}
UNION
	 SELECT
	 E.IdItem,
	 E.DataEvol,
	 E.UsuarioInsert,
	 E.UsuarioUp,
	 P.NomeInternoCrc,
	 P.Cnc
	 FROM EVOLUCAO_PSIQUIATRICA E
INNER JOIN PRONTUARIOSCRC P ON E.IdInternoCrc=P.IdInternoCrc

WHERE
     E.DataEvol BETWEEN $P{dataInicial} AND $P{dataFinal}
ORDER BY
     A.UsuarioInsert ASC,
     A.DataLanc ASC,
     P.NomeInternoCrc ASC]]>
	</queryString>
