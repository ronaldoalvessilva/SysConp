	<queryString>
		<![CDATA[SELECT NomeVisita,
       DataNasc,
       Floor(Datediff(DAY, CONVERT(DATE, DataNasc), Getdate()) / 365.25) AS Idade,
       SexoVisita,
       StatusVisita
FROM   VISITASINTERNO
WHERE  Floor(Datediff(DAY, CONVERT(DATE, DataNasc), Getdate()) / 365.25) BETWEEN $P{idadeInicial} AND $P{idadeFinal}
ORDER BY NomeVisita]]>
	</queryString>
